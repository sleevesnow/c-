#include<stdio.h>
int a[30];//保存分解项
int n;
int count=0; 
void fenjie(int index,int start,int num);

int main(){
	scanf("%d",&n);
	fenjie(0,1,n);
	return 0;
}
void fenjie(int index,int start,int num){
	int i;
	//输出 
	if(num==0){
		printf("%d=",n);
		printf("%d",a[0]);
		for(i=1;i<index;i++){
			printf("+%d",a[i]);
		}
		count++;
		if(count!=4&&a[index-1]!=n){
			printf(";");
		}
		if(count==4) {
			printf("\n");
			count=0;
		}
	}
	// 递归求分解数 
	else {
		for(i=start;i<=num;i++){
			a[index] = i;
			start = i;
			fenjie(index+1,start,num-i);
		}
	}
}
